{
  "name": "botinthewoods",
  "version": "0.1.0",
  "description": "Generates random forests and animates a brief stroll through them",
  "homepage": "https://github.com/simrisek/botinthewoods",
  "author": "Sarah Imrisek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simrisek/botinthewoods.git"
  },
  "bugs": {
    "url": "https://github.com/simrisek/botinthewoods/issues"
  },
  "scripts": {
    "render": "node --max-old-space-size=8179 bin/render.js ",
    "tweeter": "node bin/tweeter.js",
    "test": "node --max-old-space-size=11111 bin/test.js",
    "clean": "rimraf lib",
    "build": "npm run clean && tsc --build",
    "lint": "prettier --write bin/**/*.js src/**/*.js",
    "postinstall": "npm run build",
    "start": "npm run render & npm run tweeter"
  },
  "dependencies": {
    "@types/node": "^11.13.4",
    "fs": "0.0.1-security",
    "get-tweets": "^4.0.0",
    "gifencoder": "^1.1.0",
    "gifsicle": "^3.0.4",
    "imagemin": "^5.3.1",
    "imagemin-giflossy": "^5.1.10",
    "lune": "^0.4.0",
    "npm": "^6.13.4",
    "omggif": "^1.0.9",
    "path": "^0.12.7",
    "perlin-noise": "0.0.1",
    "pngjs": "^3.3.3",
    "rimraf": "^2.6.3",
    "three-software-renderer": "^1.2.0",
    "twit": "^2.2.11",
    "twitter-tokens": "^1.0.3",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
